{
  "version": "1.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "description": "weekly calendar for antd",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yoavweber/antd-weekly-calendar.git"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "antd-weekly-calendar",
  "author": "yoavweber",
  "module": "dist/antd-weekly-calendar.esm.js",
  "size-limit": [
    {
      "path": "dist/antd-weekly-calendar.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/antd-weekly-calendar.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "antd": "5.20.1",
    "date-fns": "^3.6.0",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "husky": "^9.1.4",
    "tsdx": "^0.13.3",
    "typescript": "^5.5.4"
  }
}
